{
    "name": "ewatch/price-calculator",
    "description": "Perform price calculations and create an invoice",
    "keywords": [
        "spatie",
        "price-calculator"
    ],
    "homepage": "https://github.com/spatie/price-calculator",
    "license": "MIT",
    "authors": [
        {
            "name": "Jan Fr√ºhwacht",
            "email": "ewatchhu@gmx.de",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^9.0",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Ewatch\\PriceCalculations\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ewatch\\PriceCalculations\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
